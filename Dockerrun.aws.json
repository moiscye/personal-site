{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "moiscye/site-client",
      "hostname": "client",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "server",
      "image": "moiscye/site-server",
      "hostname": "api",
      "essential": "false",
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "moiscye/multi-nginx",
      "essential": "true",
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 128
    }
  ]
}
